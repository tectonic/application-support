{
    "name": "tectonic/application-support",
    "description": "A collection of libraries and classes for supporting Laravel 4 application development.",
    "keywords": ["library", "ddd", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "orchestra/testbench": "3.0.*",
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Tectonic\\Application\\": "Tectonic/Application/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"

}
